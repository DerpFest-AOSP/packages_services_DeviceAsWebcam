<!-- Animate icon transition from filled to line. This is just ic_notif_streaming in reverse -->
<animation-list
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:oneshot="true">
    <item android:drawable="@drawable/ic_notif_filled" android:duration="50" />
    <item android:drawable="@drawable/ic_notif_line_2_filled_09" android:duration="50" />
    <item android:drawable="@drawable/ic_notif_line_2_filled_08" android:duration="50" />
    <item android:drawable="@drawable/ic_notif_line_2_filled_07" android:duration="50" />
    <item android:drawable="@drawable/ic_notif_line_2_filled_06" android:duration="50" />
    <item android:drawable="@drawable/ic_notif_line_2_filled_05" android:duration="50" />
    <item android:drawable="@drawable/ic_notif_line_2_filled_04" android:duration="50" />
    <item android:drawable="@drawable/ic_notif_line_2_filled_03" android:duration="50" />
    <item android:drawable="@drawable/ic_notif_line_2_filled_02" android:duration="50" />
    <item android:drawable="@drawable/ic_notif_line_2_filled_01" android:duration="50" />
    <item android:drawable="@drawable/ic_notif_line" android:duration="50" />
</animation-list>
